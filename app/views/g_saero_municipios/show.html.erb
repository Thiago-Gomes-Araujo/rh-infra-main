<div class="container">
  <%= render "shared/notice" %>
  <div class="card border-0 shadow shadow-sm">
    <div class="card-header border-0 d-flex justify-content-between">
      <span class="fs-4">Visualizar G Saero Municipio</span>
    </div>
    <div class="card-body">
      <div id="<%= dom_id @g_saero_municipio %>">
              
          <p>
            <strong>G Municipio:</strong>
            <%= @g_saero_municipio.g_municipio %>
          </p>
              
          <p>
            <strong>G Etapa Saero:</strong>
            <%= @g_saero_municipio.g_etapa_saero %>
          </p>
              
          <p>
            <strong>Indice Lp:</strong>
            <%= @g_saero_municipio.indice_lp %>
          </p>
              
          <p>
            <strong>Indice Mt:</strong>
            <%= @g_saero_municipio.indice_mt %>
          </p>
              
          <p>
            <strong>Created By:</strong>
            <%= @g_saero_municipio.created_by %>
          </p>
              
          <p>
            <strong>Updated By:</strong>
            <%= @g_saero_municipio.updated_by %>
          </p>
              
          <p>
            <strong>Deleted At:</strong>
            <%= @g_saero_municipio.deleted_at %>
          </p>
     
      </div>
    </div>
    <div class="card-footer border-0 d-flex justify-content-between">
      <div>
        <%= link_to edit_g_saero_municipio_path(@g_saero_municipio), class: "btn btn-primary" do %>
          <i class="bi bi-pencil-square"></i> Editar
        <% end %>
        <%= link_to "Voltar", g_saero_municipios_path, class: "btn btn-secondary" %>
      </div>
      <%= button_to "Deletar", @g_saero_municipio, method: :delete, class: "btn btn-outline-danger border-0" %>
    </div>
  </div>
</div>
