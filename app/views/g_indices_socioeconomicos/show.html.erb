<div class="container">
  <%= render "shared/notice" %>
  <div class="card border-0 shadow shadow-sm">
    <div class="card-header border-0 d-flex justify-content-between">
      <span class="fs-4">Visualizar G Indice Socioeconomico</span>
    </div>
    <div class="card-body">
      <div id="<%= dom_id @g_indice_socioeconomico %>">
              
          <p>
            <strong>Indice:</strong>
            <%= @g_indice_socioeconomico.indice %>
          </p>
              
          <p>
            <strong>G Municipio:</strong>
            <%= @g_indice_socioeconomico.g_municipio %>
          </p>
              
          <p>
            <strong>Created By:</strong>
            <%= @g_indice_socioeconomico.created_by %>
          </p>
              
          <p>
            <strong>Updated By:</strong>
            <%= @g_indice_socioeconomico.updated_by %>
          </p>
              
          <p>
            <strong>Deleted At:</strong>
            <%= @g_indice_socioeconomico.deleted_at %>
          </p>
     
      </div>
    </div>
    <div class="card-footer border-0 d-flex justify-content-between">
      <div>
        <%= link_to edit_g_indice_socioeconomico_path(@g_indice_socioeconomico), class: "btn btn-primary" do %>
          <i class="bi bi-pencil-square"></i> Editar
        <% end %>
        <%= link_to "Voltar", g_indices_socioeconomicos_path, class: "btn btn-secondary" %>
      </div>
      <%= button_to "Deletar", @g_indice_socioeconomico, method: :delete, class: "btn btn-outline-danger border-0" %>
    </div>
  </div>
</div>
